<div class="page-header">Scientific Experiments</div>
<div id="experiments">
  <ul class="information-list" id="experiments-list">
    <% @experiments.each do |experiment| %>
      <li id="experiment-<%= experiment.id %>">
        <div class="line-item-info">&ndash; <%= experiment.name %></div>
      </li>
    <% end %>
  </ul>
</div>